  <system.diagnostics>
    <trace>
      <listeners>
        <add name="console" />
      </listeners>
    </trace>

    <sharedListeners>
      <add name="console" type="Essential.Diagnostics.ColoredConsoleTraceListener, Essential.Diagnostics" 
        template="{DateTime:HH':'mm':'ssZ} [{Thread}] {EventType} {Source} {Id}: {Message}{Data}"
        convertWriteToEvent="true" />
    </sharedListeners>
    
  </system.diagnostics>
  
